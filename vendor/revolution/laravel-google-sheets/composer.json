{
  "name": "revolution/laravel-google-sheets",
  "description": "Google Sheets API v4",
  "keywords": [
    "google",
    "sheets",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^5.6.4 || ^7.0",
    "pulkitjalan/google-apiclient": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^6.0",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "GoogleSheets\\": "src/GoogleSheets/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoogleSheets\\tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "GoogleSheets\\Providers\\SheetsServiceProvider"
      ],
      "aliases": {
        "Sheets": "GoogleSheets\\Facades\\Sheets"
      }
    }
  }
}
